
.page{
  overflow: scroll;
  background: #f2f2f2;
  height:100vh;
}

.item{
  position: relative;
  width: 720rpx;
  background: #fff;
  margin: 15rpx auto 0;
}

.titlewrap{
  position: relative;
  width: 680rpx;
  height: 60rpx;
  margin: 0 auto;
  border-bottom: 1rpx #eeeeee solid;
  align-items: center;
}
.titlewrap .ordernum{
  display: inline-block;
  margin-left: 6rpx;
  color: #6a6a6a;
  font-size: 24rpx;
}
.titlewrap .order_id{
  position: absolute;
  display: inline-block;
  color: #00a7ff;
  font-size: 24rpx;
  right: 6rpx;
}

.itemwrap{
  position: relative;
  width: 680rpx;
  height: 170rpx;
  margin: 0 auto;
  margin-top: 30rpx;
  padding-bottom: 30rpx;
}
.itemlist .itemwrap:not(:last-child){
  border-bottom: 1rpx #eeeeee solid;
}
.itemwrap .itemimg{
  position: absolute;
  left: 10rpx;
  top: 0;
  width: 169rpx;
  height: 169rpx;
}
.itemwrap .namewrap{
  position: absolute;
  left: 200rpx;
  top: 0;
  width: 472rpx;
  line-height: 36rpx;
}
.itemwrap .namewrap text{
  font-size: 24rpx;
  color: #303030;
}
.itemwrap .versionwrap{
  position: absolute;
  left: 200rpx;
  top: 90rpx;
  width: 472rpx;
  line-height:21rpx;
}
.itemwrap .versionwrap text{
  font-size: 21rpx;
  color: #bbbbbb;
}
.itemwrap .pricewrap{
  position: absolute;
  left: 200rpx;
  bottom: 30rpx;
  line-height:23rpx;
}
.itemwrap .pricewrap text{
  font-size: 23rpx;
  color: #c62626;
}
.itemwrap .countwrap{
  position: absolute;
  right: 6rpx;
  bottom: 30rpx;
  line-height:23rpx;
}
.itemwrap .countwrap text{
  font-size: 23rpx;
  color: #bbbbbb;
}

.desc{
  position: relative;
  width: 720rpx;
  background: #fff;
  margin: 15rpx auto 0;
}
.desc .msgwrap{
  position: relative;
  width: 680rpx;
  height: 88rpx;
  margin: 0 auto;
  align-items: center;
}
.desc .msgwrap:not(:last-child){
  border-bottom: 1rpx #eeeeee solid;
}
.desc .msgwrap text{
  padding-left: 6rpx;
  font-size: 24rpx;
  color: #303030;
}
.desc .msgwrap text.righttext{
  position: absolute;
  right: 6rpx;
  color: #6a6a6a;
}

.personal{
  position: relative;
  width: 720rpx;
  background: #fff;
  margin: 15rpx auto 0;
}
.personal .quick{
  position: relative;
  width: 680rpx;
  height: 88rpx;
  margin: 0 auto;
  align-items: center;
  border-bottom: 1rpx #eeeeee solid;
}
.personal .quick text{
  padding-left: 6rpx;
  font-size: 24rpx;
  color: #00a7ff;
}
.personal .quick .rightimage{
  position: absolute;
  display: inline-block;
  right: 6rpx;
  width: 13rpx;
  height: 88rpx;
  display:flex;
  align-items: center;
}
.personal .quick .rightimage image{
  width: 13rpx;
  height: 22rpx;
}
.personal .msgwrap{
  position: relative;
  width: 680rpx;
  line-height: 30rpx;
  margin: 0 auto;
  padding: 28rpx 6rpx;
  box-sizing: border-box;
}
.personal .msgwrap .textwrap text{
  font-size: 24rpx;
  color: #6a6a6a;
}
.personal .msgwrap .textwrap.address{
  margin-top: 20rpx;
}

.express{
  position: relative;
  width: 720rpx;
  background: #fff;
  margin: 15rpx auto 0;
}
.express .msgwrap{
  position: relative;
  width: 680rpx;
  height: 88rpx;
  margin: 0 auto;
  align-items: center;
  border-bottom: 1rpx #eeeeee solid;
}
.express .msgwrap text{
  padding-left: 6rpx;
  font-size: 24rpx;
  color: #6a6a6a;
}
.express .msgwrap text.blue{
  color: #00a7ff;
}
.express .msgwrap text.right-text{
  position: absolute;
  right: 46rpx;
}
.express .msglist{
  position: relative;
  width: 680rpx;
  margin: 0 auto;
}
.express .msglist .expmsg{
  position: relative;
  width: 100%;
  height: 130rpx;
}
.express .msglist .expmsg .point{
  position: absolute;
  left: 126rpx;
  top: 56rpx;
  width: 16rpx;
  height: 16rpx;
  border-radius: 50%;
  border: 1rpx solid #00a7ff;
  background: #fff;
  z-index: 3;
}
.express .msglist .expmsg:not(:last-child) .point{
  background: #00a7ff;
}
.express .msglist .expmsg .topline{
  position: absolute;
  top: 0;
  left: 135rpx;
  width: 0;
  height: 65rpx;
}
.express .msglist .expmsg:not(:first-child) .topline{
  border-right: 1rpx solid #00a7ff;
}
.express .msglist .expmsg .botline{
  position: absolute;
  bottom: 0;
  left: 135rpx;
  width: 0;
  height: 65rpx;
}
.express .msglist .expmsg:not(:last-child) .botline{
  border-right: 1rpx solid #00a7ff;
}
.express .msglist .expmsg .date{
  position: absolute;
  left: 42rpx;
  top: 38rpx;
  font-size: 26rpx;
  color: #6a6a6a;
}
.express .msglist .expmsg .time{
  position: absolute;
  left: 46rpx;
  top: 68rpx;
  font-size: 24rpx;
  color: #6a6a6a;
}
.express .msglist .expmsg .descwrap{
  position: absolute;
  left: 196rpx;
  top: 0;
  width: 450rpx;
  height: 130rpx;
  font-size: 26rpx;
  color: #6a6a6a;
  display: flex;
  align-items: center;
}
.express .msglist .expmsg .descwrap text{
  font-size: 26rpx;
  color: #6a6a6a;
}

.thelast{
  margin-bottom: 105rpx;
}

.footer{
  position: fixed;
  width: 100%;
  height: 90rpx;
  left: 0;
  bottom: 0;
  background: #fff;
  z-index: 4;
}
.footer .footerwrap{
  position: relative;
  width: 100%;
  height: 90rpx;
}
.footerwrap .pay{
  position: absolute;
  right: 0;
  top: 0;
  width: 234rpx;
  height: 90rpx;
  line-height: 90rpx;
  background: #db3838;
  color: #fff;
  font-size: 24rpx;
  text-align: center;
}
.footerwrap .cancel{
  position: absolute;
  right: 234rpx;
  top: 0;
  width: 234rpx;
  height: 90rpx;
  line-height: 90rpx;
  background: #bababa;
  color: #fff;
  font-size: 24rpx;
  text-align: center;
}
.footerwrap .tips{
  height: 90rpx;
  line-height: 90rpx;
  margin-left: 20rpx;
  color: #303030;
  font-size: 24rpx;
}
.footerwrap .footbtn{
  position: absolute;
  top: 20rpx;
  width: 132rpx;
  height: 45rpx;
  line-height: 45rpx;
  border-radius: 8rpx;
  font-size: 24rpx;
  text-align: center;
}
.footerwrap .lianxi{
  background:none;
}
.footerwrap .lianxi:after{
  border:none;
}
.footerwrap .footbtn2{
  position: absolute;
  top: 20rpx;
  width: 160rpx;
  height: 45rpx;
  line-height: 45rpx;
  border-radius: 8rpx;
  font-size: 24rpx;
  text-align: center;
}
.footerwrap .footbtn.red,.footerwrap .footbtn2.red{
  border: 1rpx solid #db3838;
  color: #db3838;
}
.footerwrap .footbtn.blue,.footerwrap .footbtn2.blue{
  border: 1rpx solid #00a7ff;
  color: #00a7ff;
}
.footerwrap .rightbtn{
  right: 30rpx;
}
.footerwrap .subrightbtn{
  right: 182rpx;
}
.footerwrap .lastrightbtn{
  right: 206rpx;
}

.confirm{
  width: 100%;
  height: 100vh;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 8;
}
.confirm .black{
  width: 100%;
  height: 100vh;
  background: #000;
  opacity: 0.3;
}
.confirm .dialog{
  position: absolute;
  left: 150rpx;
  top: 50%;
  transform: translateY(-50%);
  width: 450rpx;
  height: 300rpx;
  background: #fff;
  border-radius: 8rpx;
  text-align: center;
  z-index: 9;
}
.confirm .dialog .title{
  display: block;
  width: 450rpx;
  font-size: 30rpx;
  color: #303030;
  margin-top: 58rpx;
}
.confirm .dialog .tips{
  display: block;
  width: 365rpx;
  font-size: 20rpx;
  color: #303030;
  margin: 44rpx auto 0;
}
.confirm .dialog .btn{
  width: 430rpx;
  height: 70rpx;
  margin: 20rpx auto 0;
}
.confirm .dialog .btn .btnwtap{
  height: 38rpx;
  line-height: 38rpx;
  margin: 17rpx 13rpx;
  border: 1rpx solid #d8d8d8;
  border-radius: 3rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  -webkit-tap-highlight-color: transparent;
  text-align: center;
  font-size: 20rpx;
}